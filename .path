# Fish-compatible PATH configuration
# Directories are added in reverse order (last one has highest priority)

set -gx PATH $HOME/.emacs.d/bin $PATH
set -gx PATH $HOME/.cargo/bin $PATH
set -gx PATH /opt/homebrew/opt/openjdk/bin $PATH
set -gx PATH $HOME/go/bin $PATH
set -gx PATH $HOME/.local/bin $PATH
set -gx PATH $HOME/.local/share/omarchy/bin $PATH
set -gx PATH $HOME/.claude/local $PATH
set -gx PATH /usr/local/bin $PATH
set -gx PATH /opt/homebrew/sbin $PATH
set -gx PATH /opt/homebrew/bin $PATH
